{
	"name": "vite-xls",
	"private": true,
	"version": "0.0.0",
	"main": "dist_electron/index.js",
	"scripts": {
		"clean": "rimraf dist dist_electron release",
		"dev:vite": "vite",
		"dev:electron": "wait-on http://localhost:5173 && cross-env IS_DEV=true electron .",
		"dev": "run-p dev:vite dev:electron",
		"build:vite": "vue-tsc -b && vite build",
		"build:electron": "tsc -b tsconfig.electron.json",
		"build": "run-s clean build:vite build:electron",
		"start": "electron .",
		"start:debug": "cross-env VSCODE_DEBUG=true electron --remote-debugging-port=9223 .",
		"postinstall": "electron-builder install-app-deps",
		"pack:mac": "run-s build && electron-builder --mac",
		"prebuild": "yarn clean"
	},
	"dependencies": {
		"animejs": "^3.2.2",
		"electron-updater": "^6.2.1",
		"element-plus": "^2.8.0",
		"pinia": "^2.2.2",
		"vue": "^3.4.35",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@electron/packager": "^18.3.4",
		"@types/animejs": "^3.1.12",
		"@types/semver": "^7.5.8",
		"@vitejs/plugin-vue": "^5.1.2",
		"cross-env": "^7.0.3",
		"electron": "^32.0.1",
		"electron-builder": "^24.13.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^6.0.1",
		"typescript": "^5.5.3",
		"vite": "^5.4.0",
		"vite-plugin-electron": "^0.28.7",
		"vue-tsc": "^2.0.29",
		"wait-on": "^8.0.0"
	}
}